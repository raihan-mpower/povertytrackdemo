<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.trackermenu"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.android.trackermenu.Login"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.android.trackermenu.MainActivity"></activity>
        <activity android:name="com.android.trackermenu.PersonDetails"></activity>
        <activity android:name="com.android.trackermenu.Survey"></activity> 
        <activity android:name="com.android.trackermenu.Transfer"></activity>  
        <activity android:name="com.android.trackermenu.Followups"></activity>    
        
    </application>

</manifest>
